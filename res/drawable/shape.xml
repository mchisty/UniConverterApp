<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners android:radius="6dp" />

    <gradient
        android:angle="45"
        android:centerColor="#a39f9f"
        android:centerX="2"
        android:centerY="2"
        android:endColor="#a39f9f"
        android:gradientRadius="5dp"
        android:startColor="#ffffff"
        android:type="linear"
        android:useLevel="true" />
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />

<!--     <size -->
<!--         android:height="5dp" -->
<!--         android:width="5dp" /> -->

    <solid android:color="#a39f9f" />

    <stroke
        android:dashGap="0dp"
        android:dashWidth="1dp"
        android:width="2dp"
        android:color="#91C45D" />
    
</shape>